<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>
      <i class="fa fa-rocket" style="color:#EF5350"></i>&nbsp;Price<span style="color:#EF5350">Rocket</span>
      History</h2>
    <ol class="breadcrumb">
      <li>
        <%= link_to "Home", supplier_dashboard_path%>
      </li>
    </ol>
  </div>
  <div class="col-lg-2"></div>
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-content">
        <div class="table-responsive">
          <table class="table table-striped table-bordered table-hover">
            <thead>
              <tr>
                <th>Date</th>
                <th>To</th>
                <th>Text</th>
                <th>status</th>

              </tr>
            </thead>
            <% @texts.each do |f|%>
            <tbody>
              <tr class="gradeX">
                <td class="mycell col-md-1">
                  <strong><%= f.created_at.strftime('%D%M') %></strong>
                </td>
                <td class="mycell col-md-1"><%= f.to %></td>
                <td class="mycell">
                  <strong><%= f.body %></strong>
                </td>
                <td class="mycell1 col-md-1">
                  <strong>
                    <span class="label label-success">Sent</span>
                  </strong>
                </td>

              </tr>
              <%end%>
            </tbody>

          </table>

        </div>
      </div>
    </div>
  </div>
