<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-12">
    <h2>Contacts</h2>
    <ol class="breadcrumb">
      <li>
        <%= link_to "Home", supplier_dashboard_path %>
      </li>
      <li class="active">
        <strong>Contacts</strong>

      </li>
      <%= link_to new_supplier_contact_path do %>
      <span class="btn btn-primary pull-right"><i class="fa fa-plus"></i>Add Contact</span>
      <%end%>
    </ol>




  </div>

</div>

