<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>Date</th>
            <th>Status</th>
            <th>Action<th>
            <th>87 RFG</th>
            <th>89 RFG</th>
            <th>93 RFG</th>
            <th>Low Sodium Diesel</th>
            <th>Dyed ULS Diesel</th>
            <th>Undyed ULS Diesel</th>
            <th>Heating Oil</th>
            <th>Kerosene</th>
        </tr>
    </thead>
    <tbody>
        <% @fuel_prices.each do |f| %>
        <tr>
            <td><%= f.created_at %></td>
            <td>Status</td>
            <td>Edit</td>
            <% f.fuel_products.each do |p| %>
            <% if p && p.fuel == "87 RFG"%>
            <td><%= p.price %></td>
            <% elsif p && p.fuel == "89 RFG"%>
            <td><%= p.price %></td>
            <% elsif p && p.fuel == "93 RFG"%>
            <td><%= p.price %></td>
            <% elsif p && p.fuel == "Low Sodium Diesel"%>
            <td><%= p.price %></td>
            <% elsif p && p.fuel == "Dyed ULS Diesel"%>
            <td><%= p.price %></td>
            <% elsif p && p.fuel == "Undyed ULS Diesel"%>
            <td><%= p.price %></td>
            <% elsif p && p.fuel == "Heating Oil"%>
            <td><%= p.price %></td>
            <% elsif p && p.fuel == "Kerosene"%>
            <td><%= p.price %></td>
        <% else %>
            <td>None</td>
            <%end%>
            <%end%>
            <%end%>


        </tr>
    </tbody>

</table>
