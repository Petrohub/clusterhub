<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Daily Fuel Price</h2>
    <ol class="breadcrumb">
      <li>
        <%= link_to "Home", supplier_dashboard_path%>
      </li>
      <li class="active">
        <strong>Fuel Prices</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2"></div>
</div>
<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-content">
        <div class="">
          <%= link_to "New Price", new_supplier_fuel_price_path, class: "btn btn-primary"%>
        </div>
        <table class="table table-striped table-bordered table-hover">
          <thead>
            <tr>
              <th>Date</th>
              <th>Regular</th>
              <th>Medium</th>
              <th>Premium</th>
              <th>Diesel</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody>
            <tr class="gradeX">
              <td class="mycell">Trident</td>
              <td class="mycell">
                Internet Explorer 4.0
              </td>
              <td class="mycell">Win 95+</td>
              <td class="mycell">4</td>
              <td class="mycell">X</td>
              <td class="mycell">latest</td>
            </tr>

          </tbody>

        </table>

      </div>
    </div>
  </div>
</div>
